<!DOCTYPE html>
<html>
<head>
  <title><%= full_title yield(:title) %></title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => "reload" %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => "reload" %>
</head>
<body>
<div class="container">
  <div class="col-md-6 col-md-offset-3 m-t-200">
    <%= form_for(:session, url: admin_login_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email, class: "form-control", value: "nvt94.ptit@gmail.com" %>

      <%= f.label :password %>
      <%= f.password_field :password, class: "form-control", value: "xxxxxx" %>

      <%= f.submit "Log in", class: "btn btn-primary m-t-15" %>
    <% end %>
  </div>
</div>
</body>
</html>
